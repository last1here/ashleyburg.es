{
  "name": "@last1here/site",
  "version": "0.0.1",
  "description": "A starter repository for a blog web site using the Eleventy static site generator.",
  "scripts": {
    "build": "cross-env ELEVENTY_ENV=prod eleventy",
    "watch": "cross-env ELEVENTY_ENV=dev eleventy --watch",
    "serve": "cross-env ELEVENTY_ENV=dev eleventy --serve --incremental",
    "serve:prod": "cross-env ELEVENTY_ENV=prod npx @11ty/eleventy --serve",
    "debug": "DEBUG=* eleventy",
    "start-netlify": "netlify-cms-proxy-server"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/11ty/eleventy-base-blog.git"
  },
  "author": {
    "name": "Ash Burgess",
    "email": "me@ashleyburg.es",
    "url": "https://ashleyburg.es/"
  },
  "license": "MIT",
  "private": true,
  "bugs": {
    "url": "https://github.com/last1here/ashleyburg.es/issues"
  },
  "homepage": "https://github.com/last1here/ashleyburg.es#readme",
  "devDependencies": {
    "@11ty/eleventy": "^0.11.0",
    "@11ty/eleventy-navigation": "^0.1.5",
    "@11ty/eleventy-plugin-rss": "^1.0.7",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.1",
    "clean-css": "^4.2.3",
    "cross-env": "^7.0.2",
    "html-minifier": "^4.0.0",
    "luxon": "^1.24.1",
    "markdown-it": "^10.0.0",
    "markdown-it-anchor": "^5.2.7",
    "markdown-it-imsize": "^2.0.1",
    "netlify-cms-proxy-server": "^1.2.6",
    "terser": "^4.6.13"
  },
  "dependencies": {}
}
