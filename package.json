{
  "name": "@last1here/site",
  "version": "0.0.1",
  "description": "A starter repository for a blog web site using the Eleventy static site generator.",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_ENV=production eleventy",
    "watch": "cross-env NODE_ENV=dev eleventy --watch",
    "serve": "cross-env NODE_ENV=dev eleventy --serve --incremental",
    "serve:prod": "cross-env NODE_ENV=production eleventy --serve",
    "debug": "DEBUG=* eleventy",
    "start-netlify": "netlify-cms-proxy-server"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/11ty/eleventy-base-blog.git"
  },
  "author": {
    "name": "Ash Burgess",
    "email": "me@ashleyburg.es",
    "url": "https://ashleyburg.es/"
  },
  "license": "MIT",
  "private": true,
  "bugs": {
    "url": "https://github.com/last1here/ashleyburg.es/issues"
  },
  "homepage": "https://github.com/last1here/ashleyburg.es#readme",
  "devDependencies": {
    "@11ty/eleventy": "^3.0.0",
    "@11ty/eleventy-navigation": "^1.0.0",
    "@11ty/eleventy-plugin-rss": "^1.2.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.0",
    "cross-env": "^7.0.3",
    "cssnano": "^6.0.5",
    "html-minifier": "^4.0.0",
    "luxon": "^3.4.4",
    "markdown-it": "^14.0.0",
    "markdown-it-anchor": "^8.6.7",
    "markdown-it-imsize": "^2.0.1",
    "netlify-cms-proxy-server": "^1.3.24",
    "postcss": "^8.4.35",
    "postcss-css-variables": "^0.19.0",
    "shimmer": "^1.2.1",
    "terser": "^5.29.1",
    "workbox-build": "^7.0.0"
  },
  "dependencies": {}
}
